<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BListView">

    <ListView
        android:id="@+id/lv_entrenador"
        android:layout_width="337dp"
        android:layout_height="234dp"
        android:layout_marginTop="104dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_anadir_item_lv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Anadir"
        app:layout_constraintEnd_toEndOf="@+id/lv_entrenador"
        app:layout_constraintStart_toStartOf="@+id/lv_entrenador"
        app:layout_constraintTop_toBottomOf="@+id/lv_entrenador" />
</androidx.constraintlayout.widget.ConstraintLayout>